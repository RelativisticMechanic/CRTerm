        [10, 10, 10],
        [0, 0, 85], 
        [0, 85, 0],
        [0, 85, 85],
        [85, 0, 0],
        [85, 0, 85],
        [85, 128, 0],
        [85, 85, 85],
        [42, 42, 42],
        [42, 42, 255],
        [42, 128, 42],
        [42, 128, 128],
        [128, 42, 42],
        [128, 42, 128],
        [128, 128, 0],
        [128, 128, 128] 
    ]
}

