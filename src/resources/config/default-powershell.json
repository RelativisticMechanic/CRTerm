{
    "font": "ProggySquare.ttf",
    "font_scale": 1.5,
    "console_width": 90,
    "console_height": 30,
    "background": "background.png",
    "text_shader": "shaders/text",
    "crt_shader": "shaders/crt",
    "crt_warp": 1.00,
    "shell_command": "pwsh.exe -WorkingDirectory ~",
    "blink_interval": 300,
    "default_fg": 3,
    "default_bg": 3,
    "maxlines": 1000,
    "fullscreen": false,

    "color_scheme":
    [
        [80, 80, 80],
        [0, 0, 170], 
        [0, 170, 0],
        [0, 170, 170],
        [170, 0, 0],
        [170, 0, 170],
        [170, 128, 0],
        [170, 170, 170],
        [85, 85, 85],
        [85, 85, 255],
        [85, 255, 85],
        [85, 255, 255],
        [255, 85, 85],
        [255, 85, 255],
        [255, 255, 0],
        [255, 255, 255] 
    ]
}

