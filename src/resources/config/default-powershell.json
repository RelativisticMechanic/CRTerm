{
    "font": "font.png",
    "font_width": 8,
    "font_height": 16,
    "font_scale": 1.5,
    "console_width": 90,
    "console_height": 30,
    "background": "crt.png",
    "text_shader": "shaders/text",
    "crt_shader": "shaders/crt",
    "crt_warp": 1.00,
    "shell_command": "pwsh.exe -WorkingDirectory ~",
    "blink_interval": 300,
    "default_fg": 2,
