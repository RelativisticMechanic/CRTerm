{
    "font": "VGA437.ttf",
    "font_scale": 1.2,
    "console_width": 120,
    "console_height": 40,
    "opacity": 0.9,
    "background": "background.png",
    "text_shader": "shaders/text",
    "crt_shader": "shaders/crt",
    "crt_warp": 0.75,
    "shell_command": "wsl.exe ~",
    "blink_interval": 300,
    "default_fg": 6,
    "default_bg": 6,
    "maxlines": 1000,
    "fullscreen": false,

    "color_scheme":
    [
        [80, 80, 80],
        [0, 0, 170], 
        [0, 170, 0],
        [0, 170, 170],
        [170, 0, 0],
        [170, 0, 170],
        [170, 128, 0],
        [170, 170, 170],
        [85, 85, 85],
        [85, 85, 255],
        [85, 255, 85],
        [85, 255, 255],
        [255, 85, 85],
        [255, 85, 255],
        [255, 255, 0],
        [255, 255, 255] 
    ]
}